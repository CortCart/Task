<Window x:Class="Taskmgr.Infow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Taskmgr"
        mc:Ignorable="d"
          ResizeMode="NoResize"
        Title="Input" Height="285.526" Width="496.053" Background="#FF5D5858">
    <Grid Loaded="Grid_Loaded">
        <TextBox x:Name="Id" HorizontalAlignment="Left" Height="56" Margin="70,54,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="359" TextChanged="Id_TextChanged" Loaded="Id_Loaded" FontSize="24"/>
        <Button x:Name="Close" Content="OK" HorizontalAlignment="Left" Margin="70,135,0,0" VerticalAlignment="Top" Width="359" Click="Button_Click" Height="69" FontSize="36"/>

    </Grid>
</Window>
